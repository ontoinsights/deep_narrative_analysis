  rdfs:subClassOf :ReligiousBelief ;
  rdfs:label "Judaism" ;
  :noun_synonym "Jew", "Zionist", "Hebrew" .

:CameoReligion_MOS a owl:Class ;
  rdfs:subClassOf :ReligiousBelief ;
  rdfs:label "Islam" ;
  :noun_synonym "Muslim" .

:CameoReligion_MOSSFI a owl:Class ;
  rdfs:subClassOf :CameoReligion_MOS ;
  rdfs:label "Sufi" .

:CameoReligion_MOSSHI a owl:Class ;
