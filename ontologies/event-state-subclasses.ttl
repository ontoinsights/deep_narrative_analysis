  :wordnet_verb_synset "01746607", "01383909", "02425530", "00056076", "01322669" .

:Alignment a owl:Class ;
  rdfs:subClassOf :InclusionAttachmentAndUnification ;
  rdfs:label "Alignment"@en ;
