  :plant_hazards "None known"@en ;
  :plant_habitat "Not known"@en ;
  :plant_country_range "S. America - Peru, Ecuador, Colombia, Venezuela, Guyana, French Guiana; C. America - Panama, Costa Rica, Nicaragua, Guatemala, Mexico."@en ;
