  rdfs:subClassOf :Violaceae ;
  :synonym "heartsease"@en, "johnny jumpup"@en, "field pansy"@en, "johnny jump up"@en, "heart's ease"@en, "heart's delight"@en, "tickle-my-fancy"@en, "jack-jump-up-and-kiss-me"@en, "come-and-cuddle-me"@en, "three faces in a hood"@en, "love-in-idleness"@en, "wild pansy"@en .
