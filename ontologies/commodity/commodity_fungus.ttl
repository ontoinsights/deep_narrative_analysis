########################################################################
# Classes
########################################################################

:Mushroom a owl:Class ;
  rdfs:subClassOf :Fungus ;
  rdfs:comment "UN Harmonized System code 0709 (mushroom, truffle)"@en ;
  :definition "A type of Fungus that typically has a stem with a domed cap and gills on the underside of the cap."@en ;
  :synonym "toadstool"@en, "truffle"@en, "chanterelle"@en, "portobello"@en, "cremini"@en, "baby-bella"@en, "shiitake"@en, "enoki"@en, "enokitake"@en, "Jew's ear"@en, "wood ear"@en, "jelly ear"@en, "snow fungus"@en, "snow ear"@en, "silver ear fungus"@en, "shimeji"@en, "porcini"@en, "blewit"@en, "puffball"@en, "morel"@en, "matsutake"@en .

:Yeast a owl:Class ;
  rdfs:subClassOf :Fungus ;
  rdfs:comment "Often times, 'Yeast' is synonymous with the particular genus, Saccharomyces, whose species are used for fermentation, in the production of bread, beer and wine."@en .


