########################################################################
# Datatype Properties
########################################################################

:reported_value a owl:DatatypeProperty, owl:FunctionalProperty ;
  rdfs:label "reported value"@en ;
  :definition "Specifies the value of a Measurement as a decimal (where the value's units are defined using the :has_unit predicate) or a string."@en ;
  rdfs:domain :Measurement ;
  rdfs:range [ a rdfs:Datatype ;
               owl:unionOf ( xsd:decimal xsd:string) ].


########################################################################
# Object Properties
########################################################################

:defined_for a owl:ObjectProperty, owl:FunctionalProperty ;
  rdfs:subPropertyOf :has_topic ;
  rdfs:label "defined for"@en ;
  :definition "A relationship indicating that a Measurement concerns/is 'about' the referenced instance."@en ;
  rdfs:domain :Measurement .

:has_assessment a owl:ObjectProperty, owl:FunctionalProperty ;
  rdfs:subPropertyOf :reference_to_enumerated_value ;
  rdfs:label "has assessment"@en ;
  :definition "Assigns an AssessmentLevel to an AssessmentMeasurement."@en ;
  rdfs:domain :AssessmentMeasurement ;
  rdfs:range :AssessmentLevel .

:has_unit a owl:ObjectProperty, owl:FunctionalProperty ;
  rdfs:label "has unit of measure"@en ;
  :definition "Associates the units of a Measurement's :reported_value. Units are defined by QUDT.org and can be searched at http://www.qudt.org/doc/DOC_VOCAB-UNITS.html."@en ;
  rdfs:domain :Measurement ;
  rdfs:range <http://qudt.org/schema/qudt/Unit> .


