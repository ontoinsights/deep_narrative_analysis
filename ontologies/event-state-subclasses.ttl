  :noun_synonym "abstinence", "abstaining", "refraining", "avoiding" ;
  :verb_synonym "refrain", "avoid", "abstain", "forbear" ;
  :wordnet_noun_synset "00204602", "01070697", "04889817", "01068613" ;
  :wordnet_verb_synset "00813037", "01198570", "02731589" .

:Birth a owl:Class ;
