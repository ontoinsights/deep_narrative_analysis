  :wordnet_noun_synset "00103599", "13476660" ; 
  :wordnet_verb_synset "01751525" .  

:IssuingAndPublishing a owl:Class ;
  rdfs:subClassOf :EventAndState ;
  rdfs:label "Issuing and Publishing"@en ;
  :definition "An act of issuing/releasing an InformationSource such as a book, article, newspaper, etc., a work of art or music, ..., or making an announcement/releasing a public statement."@en ;
  :wordnet_verb_synset "00956640" .

:LegalEvent a owl:Class ;
  rdfs:subClassOf :EventAndState ;
  rdfs:label "Legal Event"@en ;
  :definition "Any activity based on following legal practices, or related to or allowed by laws or the legal system."@en ;
  :wordnet_noun_synset "01183965", "01187304", "01183047", "01183348", "01183702", "01081986", "01204674", "08431076", "06660952", "01194465" ;
  :wordnet_verb_synset "02588535", "01016830", "02267740", "02503473", "00762621", "00896020", "00908688", "01015049" .

:LifeEvent a owl:Class ;
  rdfs:subClassOf :EventAndState ;
  rdfs:label "Life Event"@en ;
  :definition "Any occurrence in an Agent's existence, such as birth, death, marriage, etc."@en .

:Loss a owl:Class ;
