########################################################################
# Datatype Properties
########################################################################

:assessment a owl:DatatypeProperty, owl:FunctionalProperty ; 
  rdfs:subPropertyOf :reported_value ;
  rdfs:label "assessment"@en ;
  rdfs:comment "A restricted set of string values indicating how an AssessmentMeasurement for an entity (typically, a sub-class of EnvironmentAndCondition, such as Employment in a Country) is qualitatively evaluated - ranging from 'negligible or none' to 'very high'."@en ;
  rdfs:domain :AssessmentMeasurement ;
  rdfs:range [ a rdfs:Datatype ;
               owl:onDatatype xsd:string ;
               owl:oneOf  ('high' 'low' 'medium' 'negligible or none' 'very high' ) ] .

:reported_value a owl:DatatypeProperty, owl:FunctionalProperty ;
  rdfs:label "reported value"@en ;
  rdfs:comment "Specifies the value of a Measurement as a decimal (where the value's units are defined using the :has_unit predicate) or a string."@en ;
  rdfs:domain :Measurement ;
  rdfs:range [ a rdfs:Datatype ;
               owl:unionOf ( xsd:decimal xsd:string) ].


########################################################################
# Object Properties
########################################################################

:has_unit a owl:ObjectProperty, owl:FunctionalProperty ;
  rdfs:label "has unit of measure"@en ;
  rdfs:comment "Associates the units of a Measurement's :reported_value. Units are defined by QUDT.org and can be searched at http://www.qudt.org/doc/DOC_VOCAB-UNITS.html."@en ;
  rdfs:domain :Measurement ;
  rdfs:range <http://qudt.org/schema/qudt/Unit> .
