# Extracted from https://en.wikipedia.org/w/index.php?title=List_of_metonyms&oldid=1124677755

# North America
:CapitolHill a :PhysicalLocation, :GovernmentalEntity ;
  rdfs:label "Capital Hill"@en ;
  :definition "A neighborhood in Washington, D.C., and a reference to the United States Congress"@en ;
  :has_component :HouseOfRepresentatives, :Senate .

:FoggyBottom a :PhysicalLocation, :GovernmentalEntity ;
  rdfs:label "Foggy Bottom"@en ;
  :definition "A neighborhood in Washington, D.C., and a reference to the United States Department of State"@en .

:Hollywood a :PhysicalLocation, :LineOfBusiness ;
  rdfs:label "Hollywood"@en ;
  :has_line_of_business :EntertainmentBusiness ;
  :definition "A district of Los Angeles, California, and a metonym for the American film industry"@en .

:HouseOfRepresentatives a :GovernmentalEntity ;
  rdfs:label "House of Representatives"@en ;
  :definition "A part of the legislative branch of the United States government, part of 'Capital Hill'."@en .

:KStreet a :PhysicalLocation, :LineOfBusiness ;
  rdfs:label "K Street"@en ;
  :has_line_of_business :GovernmentAndPoliticsBusiness ;
  :definition "A street in downtown Washington, D.C., and a reference to the American lobbying industry"@en .

:Langley a :PhysicalLocation, :GovernmentalEntity ;
  rdfs:label "Langley"@en ;
  :definition "	A small suburb of Washington, D.C., and a reference to the U.S. Central Intelligence Agency."@en .

:MadisonAvenue a :PhysicalLocation, :LineOfBusiness ;
  rdfs:label "Madison Avenue"@en ;
  :has_line_of_business :ProvisionOfServicesBusiness ;
  :definition "A street in Manhattan, New York City, and a reference to the American advertising industry."@en .

:MainStreet a :DesignatedArea ;
  rdfs:label "Main Street"@en ;
  :definition "A shopping area of a town, traditionally the site of shops, banks, and local businesses."@en .

:Ottawa a :PhysicalLocation, :GeopoliticalEntity ;
  rdfs:label "Ottawa, Canada"@en ;
  :definition "The capital of Canada, and a reference to the Government of Canada."@en .

:Pentagon a :PhysicalLocation, :GovernmentalEntity ;
  rdfs:label "Pentagon"@en ;
  :definition "	A pentagonal building in Arlington County, Virginia, and a reference to	the United States Department of Defense (whose headquarters is housed by the Pentagon building)."@en .

:Senate  a :GovernmentalEntity ;
  rdfs:label "Senate"@en ;
  :definition "A part of the legislative branch of the United States government, part of 'Capital Hill'."@en .

:SiliconValley a :PhysicalLocation, :LineOfBusiness ;
  rdfs:label "Silicon Valley"@en ;
  :has_line_of_business :TechnologyBusiness ;
  :definition "San Jose and its suburbs on the southwest side of San Francisco Bay, and a reference to the American high-tech industry."@en .

:WallStreet	a :PhysicalLocation, :LineOfBusiness ;
  rdfs:label "Wall Street"@en ;
  :has_line_of_business :BankingAndFinanceBusiness ;
  :definition "A street in Manhattan, New York City, and a reference to the American financial markets or 'big business' more generally."@en .

:WhiteHouse	a :PhysicalLocation, :GovernmentalEntity ;
  rdfs:label "White House"@en ;
  :definition "The official residence of the president of the United States, and a reference to	the Executive Office of the President of the United States."@en .

# Europe
:DowningStreet a :PhysicalLocation, :GeopoliticalEntity ;
  rdfs:label "Downing Street"@en ;
  :definition "The official residence of the prime minister of the United Kingdom, and a reference to the prime minister and his/her staff (often meaning more broadly the UK Government)."@en .

:Brussels a :PhysicalLocation, :GeopoliticalEntity ;
  rdfs:label "Brussels"@en ;
  :definition "The capital of Belgium, and a reference to the government of the European Union."@en .

:BuckinghamPalace a :PhysicalLocation, :GovernmentalEntity ;
  rdfs:label "Buckingham Palace"@en ;
  :definition "A large building in London that is the official residence of the reigning British sovereign, and a reference to the British royal family and its staff."@en .

:FleetStreet a :PhysicalLocation, :LineOfBusiness ;
  rdfs:label "Fleet Street"@en ;
  :has_line_of_business :PublishingAndMediaBusiness ;
  :definition "A street in the City of London, and a reference to the British national press."@en .

:Hague a :PhysicalLocation, :GovernmentalEntity ;
  rdfs:label "Hague"@en ;
  :definition "A city in the Netherlands, and a reference to the International Criminal Court or International Court of Justice, both of which have their seat in the city."@en .

:Holyrood a :PhysicalLocation, :GeopoliticalEntity ;
  rdfs:label "Holyrood"@en ;
  :definition "An area in Edinburgh, and a reference to the Scottish Parliament (or more generally, the Scottish Government), which is located in that area."@en .

:Hrad a :PhysicalLocation, :GeopoliticalEntity ;
  rdfs:label "Hrad"@en ;
  :definition "The Prague Castle and official residence and office of the president of the Czech Republic. Also a reference to the president of the Czech Republic and his or her staff, and/or the Czech Republic as a whole."@en .

:Kremlin a :PhysicalLocation, :GeopoliticalEntity ;
  rdfs:label "Kremlin"@en ;
  :definition "The Moscow Kremlin and/or the Russian presidential administration; historically, any Russian or Soviet government."@en .

:ScotlandYard a :PhysicalLocation, :PoliceForce ;
  rdfs:label "Scotland Yard"@en ;
  :definition "The London or British police, especially detectives."@en .

:Westminster a :PhysicalLocation, :GovernmentalEntity ;
  rdfs:label "Westminster"@en ;
  :definition "A part of Central London, England, and a reference to the Parliament of the United Kingdom."@en .

:Whitehall a :PhysicalLocation, :GovernmentalEntity ;
  rdfs:label "Whitehall"@en ;
  :definition "A street in the City of Westminster borough of London, and a reference to the Civil Service of the United Kingdom, or more generally the Government of the United Kingdom. The term is used similarly to 'Westminster'."@en .
