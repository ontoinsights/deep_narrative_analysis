########################################################################
# Classes
########################################################################

:Continuation a owl:Class ;
  rdfs:subClassOf :EventAndState ;
  rdfs:label "Continuation"@en ;
  :definition "Indication that an ongoing EventAndState is still occurring - regardless of whether the actual time is known."@en ;
  :wordnet_noun_synset "01019929" ;
  :wordnet_verb_synset "00118331", "00783045" .

:End a owl:Class ;
  rdfs:subClassOf :EventAndState ;
  rdfs:label "End"@en ;
  :definition "The final point of an ongoing EventAndState - regardless of whether the actual end time is known."@en ;
  :wordnet_noun_synset "00210792" ;
  :wordnet_verb_synset "00353480" .

:FailureAndLoss a owl:Class ;
  rdfs:subClassOf :End ;
  rdfs:label "Failure and Loss"@en ;
  :definition "A bad outcome or the inability to achieve a desired outcome - regardless of whether the actual failure time is known."@en ;
  :wordnet_noun_synset "00067036" ;
  :wordnet_verb_synset "02534046" .

:Negation a owl:Class ;
  rdfs:subClassOf :EventAndState ;
  rdfs:label "Negation"@en ;
  :definition "Indicates that the EventAndState semantics that are also defined for an occurrence does NOT exist, did NOT occur/has NOT happened, etc. Note that there is also a :negation data property. An EventAndState may semantically indicate a negated concept (for example, Negation and SanitaryCondition indicates that something may be unsanitary). Then, if a sentence states that the 'unsanitary' concept is negated (saying, for example, that this is 'not an unsanitary condition'), both the negation property and the Negation class would be observed in the resulting knowledge graph."@en ;
  :wordnet_noun_synset "06764286", "07295646" .

:StartAndBeginning a owl:Class ;
  rdfs:subClassOf :EventAndState ;
  rdfs:label "Start and Beginning"@en ;
  :definition "The originating point of an ongoing EventAndState - regardless of whether the actual start time is known."@en ;
  :wordnet_noun_synset "00236302" ;
  :wordnet_verb_synset "00346415", "00349400" .

:SuccessAndWinning a owl:Class ;
  rdfs:subClassOf :End ;
  rdfs:label "Success and Winning"@en ;
  :definition "A good outcome or the accomplishment of an EventAndState - regardless of whether the actual end time is known."@en ;
  :wordnet_noun_synset "00064472", "00035910" ;
  :wordnet_verb_synset "02024442" .


########################################################################
# Datatype Properties
