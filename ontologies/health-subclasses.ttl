########################################################################
# Classes
########################################################################

:Addiction a owl:Class ;
  rdfs:subClassOf :HealthAndDiseaseRelated ;
  rdfs:label "Addiction" ;
  :definition "A condition related to being dependent on (and unable to stop taking/using) a substance such as drugs or alcohol, or an activity (such as being addicted to video gaming or online activity). The substance or activity is indicated using the :has_topic property." ;
  :noun_synonym "alcoholism" ;
  :wordnet_noun_synset "14086207" .

:AnimalDisease a owl:Class ;
  rdfs:subClassOf :HealthAndDiseaseRelated ;
  rdfs:label "Animal Disease" ;
  :definition "An illness that affects animals, but typically does NOT affect humans." ; 
  :wordnet_noun_synset "14276606" .

:BodilyInjury a owl:Class ;
  rdfs:subClassOf :HealthAndDiseaseRelated ;
  rdfs:label "Bodily Injury" ;
  :definition "Damage/trauma to all or a part of a Person's body." ;
  :example "Examples of BodilyInjury are concussions, fractures, burns, cuts, bruises, etc." ;
  :noun_synonym "trauma" ;
  :wordnet_noun_synset "14309164" .

:ConvulsionAndSpasm a owl:Class ;
  rdfs:subClassOf :HealthAndDiseaseRelated ;
  rdfs:label "Convulsion and Spasm" ;
  :definition "Sudden contraction of muscles or constriction of an organ." ;
  :wordnet_noun_synset "14106077", "00226974" .

:Deformity a owl:Class ;
  rdfs:subClassOf :HealthAndDiseaseRelated, :AgentAspect ;
  rdfs:label "Deformity" ;
  :definition "A condition where a part of the body is disfigured, distored, misshapen or malformed." ;
  :noun_synonym "malformation", "disfigurement", "malformation", "misshapenness" ;
  :wordnet_noun_synset "14236994" .

:Dehydration a owl:Class ;
  rdfs:subClassOf :HealthAndDiseaseRelated ;
  rdfs:label "Dehydration" ;
  :definition "A condition where the amount of water in a body is reduced to a harmful/dangerous level." ;
  :noun_synonym "thirst", "polydipsia", "thirsting", "thirstiness" ;
  :verb_synonym "thirst", "be thirsty" ;
  :wordnet_noun_synset "14064328", "14064142" ;
  :wordnet_verb_synset "02126525" .

:Disability a owl:Class ;
  rdfs:subClassOf :HealthAndDiseaseRelated, :AgentAspect ;
  rdfs:label "Disability" ;
  :definition "A physical or mental condition affecting an individual's ability to move, and perform daily tasks and activities." ;
  :noun_synonym "disablement", "handicap", "impairment" ;
  :wordnet_noun_synset "14571935" .

:DiseaseAndInfection a owl:Class ;
  rdfs:subClassOf :HealthAndDiseaseRelated ;
  rdfs:label "Disease and Infection" ;
  :definition "An illness with one or more specific Symptoms, that is not the result of injury. An infectious disease is transmitted by bacteria, viruses, parasites or fungi." ;
  :wordnet_noun_synset "14093842", "14075399", "14198031" .

:Disorder a owl:Class ;
  rdfs:subClassOf :HealthAndDiseaseRelated, :AgentAspect ;
  rdfs:label "Disorder" ;
  :definition "A condition characterized by abnormal functioning of physical or mental processes." ;
  :wordnet_noun_synset "14075885" .

:Frailty a owl:Class ;
  rdfs:subClassOf :HealthAndDiseaseRelated ;
  rdfs:label "Frailty" ;
  :definition "A condition where an individual's body is abnormally weakened or weakened due to old age." ;
  :noun_synonym "infirmity", "frailness", "feebleness" ;
  :wordnet_noun_synset "14571235" .

:HypersensitivityAndAllergicReaction a owl:Class ;
  rdfs:subClassOf :HealthAndDiseaseRelated ;
  rdfs:label "Hypersensitivity and Allergic Reaction" ;
  :definition "An intense reaction to the environment (such as the cold) and/or an allergen (such as pollen or certain foods). Reactions can range from mild to severe and can result in anaphylactic shock and death." ;
  :noun_synonym "allergy" ;
  :wordnet_noun_synset "14557398" .

:Intoxication a owl:Class ;
  rdfs:subClassOf :Poisoning ;
  rdfs:label "Intoxication" ;
  :definition "A poisoning induced by drugs, alcohol or other substances." ;
  :noun_synonym "drunkenness", "inebriation", "inebriety", "tipsiness", "insobriety" ;
  :wordnet_noun_synset "14042038" .

:ObstructionAndConcretion a owl:Class ;
  rdfs:subClassOf :HealthAndDiseaseRelated ;
  rdfs:label "Obstruction and Concretion" ;
  :definition "A blockage or the formation of a hard, solid mass in some part of the body. The 'hard, solid mass' is sometimes referred to a 'stone', 'calculus' or 'concretion'." ;
  :noun_synonym "intestinal obstruction", "ureteral obstruction", "gallstone", "calculus" ;
  :wordnet_noun_synset "14531751", "09253675" .

:PlantDisease a owl:Class ;
  rdfs:subClassOf :HealthAndDiseaseRelated ;
  rdfs:label "Plant Disease" ;
  :definition "A disease that affects plants." ; 
  :wordnet_noun_synset "14300438" .

:Poisoning a owl:Class ;
  rdfs:subClassOf :HealthAndDiseaseRelated ;
  rdfs:label "Poisoning" ;
  :definition "A condition resulting from the administration/ingestion of a toxic substance." ;
  :wordnet_noun_synset "14533314" .

:StarvationHungerAndMalnutrition a owl:Class ;
  rdfs:subClassOf :HealthAndDiseaseRelated, :FoodAndNutrition ;
  rdfs:label "Starvation, Hunger and Malnutrition" ;
  :definition "A condition where an individual is unable to obtain sufficient, nutritious food and/or that access to healthy and nutritious foods are NOT available or NOT accessible by a population/GroupOfAgents. The overall condition of starvation/famine can be described using the :has_assessment predicate." ;
  :noun_synonym "famine", "ravenousness", "starving", "hungriness", "undernourishment", "malnourishment" ;
  :verb_synonym "starve", "be hungry", "be famished", "be ravenous", "famish" ;
  :wordnet_noun_synset "01153157", "14223182", "14063016", "07330390" ;
  :wordnet_verb_synset "01190677", "02126423", "01190070", "01190481", "01190273" .

:Sterility a owl:Class ;
  rdfs:subClassOf :HealthAndDiseaseRelated ;
  rdfs:label "Sterility" ;
  :definition "A condition where offspring cannot be conceived either due to female issues with her eggs or reproductive organs, or male issues with sperm." ;
  :noun_synonym "impotence", "infertility" ;
  :wordnet_noun_synset "14068989" .

:Symptom a owl:Class ;
  rdfs:subClassOf :HealthAndDiseaseRelated ;
  rdfs:label "Symptom" ;
  :definition "A physical or mental sensation or change in functioning which is associated with/can indicate a disease or illness." ;
  :wordnet_noun_synset "14323139" . 


########################################################################
# Datatype Properties
########################################################################


########################################################################
# Object Properties
########################################################################

:indicated_by a owl:ObjectProperty ;
  rdfs:label "indicated by" ;
  :definition "Relationship between a disease, disorder, illness. etc. and its Symptoms." ;
  rdfs:domain :HealthAndDiseaseRelated ;
  rdfs:range :Symptom .
