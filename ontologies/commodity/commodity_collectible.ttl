@prefix : <urn:ontoinsights:ontology:dna:> .
@prefix dna: <urn:ontoinsights:ontology:dna:> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

########################################################################
# File defining semantics related to works of art and other 
#    collectible items
# The classes reference entities in code 97 of the UN Commodities list
#    (https://comtrade.un.org/db/mr/rfCommoditiesList.aspx)
# 
# Created: February 9, 2020
# Last modified: February 9, 2020
########################################################################


########################################################################
# Classes
########################################################################

:Antique a owl:Class ;
  rdfs:subClassOf :ArtAndCollectionPiece ;
  :definition "An object such as furniture that is valuable because of its age."@en ;
  rdfs:comment "UN Harmonized System code 9706"@en ;
  :synonym "engraving"@en , "print"@en, "lithograph"@en .

:Coinage a owl:Class ;
  rdfs:subClassOf :ArtAndCollectionPiece ;
  rdfs:comment "UN Harmonized System code 7118 (coin)"@en ;
  :synonym "coin"@en, "doubloon"@en .

:EngravingAndPrint a owl:Class ;
  rdfs:subClassOf :ArtAndCollectionPiece ;
  :definition "Engravings, prints and lithographs."@en ;
  rdfs:comment "UN Harmonized System code 9702"@en ;
  :synonym "engraving"@en , "print"@en, "lithograph"@en .

:PaintingAndDrawing a owl:Class ;
  rdfs:subClassOf :ArtAndCollectionPiece ;
  :definition "Paintings, drawings, pastels, etc., executed by hand."@en ;
  rdfs:comment "UN Harmonized System code 9701"@en ;
  :synonym "painting"@en, "drawing"@en, "pastel"@en, "sketch"@en .

:Postage a owl:Class ;
  rdfs:subClassOf :ArtAndCollectionPiece ;
  :definition "Stamps, postage or revenue; stamp-postmarks, first-day covers, postal stationery (stamped paper) and the like."@en ;
  rdfs:comment "UN Harmonized System code 9704"@en ;
  :synonym "stamp"@en, "stamp-postmark"@en, "postmark"@en, "first-day cover"@en, "postal stationery"@en .

:SculptureAndStatuary a owl:Class ;
  rdfs:subClassOf :ArtAndCollectionPiece ;
  :definition "Sculptures and statuary."@en ;
  rdfs:comment "UN Harmonized System code 9703"@en ;
  :synonym "sculpture"@en , "statuary"@en .

:SpecimenCollection a owl:Class ;
  rdfs:subClassOf :ArtAndCollectionPiece ;
  :definition "Collections and collectors' pieces of zoological, botanical, mineralogical, anatomical, historical, archaeological, palaeontological, ethnographic or numismatic interest."@en ;
  rdfs:comment "UN Harmonized System code 9705"@en .


