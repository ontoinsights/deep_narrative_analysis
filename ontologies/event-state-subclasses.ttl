  :wordnet_noun_synset "00431125", "00491361", "00494042", "00494319", "00495779", "00492724" ;
  :wordnet_verb_synset "01140949", "02550045", "01079346" . 

:Grooming a owl:Class ;
  rdfs:subClassOf :BodilyAct ;
  rdfs:label "Grooming"@en ;
  :definition "An activity related to the cleaning, brushing, etc. of an animal's or Person's body."@en ;
  :wordnet_noun_synset "00258095", "00662026", "00662200", "00829898" ;
  :wordnet_verb_synset "00293903" .

:HealthAndDiseaseRelated a owl:Class ;
  rdfs:subClassOf :EventAndState ;
  rdfs:label "Health and Disease Related"@en ;
  :definition "Any event or condition concerning disease, disorders, illness, etc. (and recovery from such) by humans, plants and animals. The diseases, illnesses, etc. are indicated using the :has_topic property. Note that several sub-classes are also typed as an AgentAspect to allow them to define 'some facet, detail or characteristic of a Person or group of Persons that serves to distinguish and/or describe them'. When used in that sense, the instance is related to a Person using the :has_agent_aspect property."@en ;
  :wordnet_noun_synset "04081594", "00830651", "14075528" ; 
  :wordnet_verb_synset "00081478", "00086587" .

:ImitationAndCounterfeit a owl:Class ;
  rdfs:subClassOf :EventAndState ;
  rdfs:label "Imitation and Counterfeit"@en ;
  :definition "An act of simulating or making a copy or duplicate of something or someone. If done to deceive, then this is counterfeiting."@en ;
  :wordnet_noun_synset "03109245" ;
  :wordnet_verb_synset "01657814", "01697776", "01740703" .

:ImpactAndContact a owl:Class ;
  rdfs:subClassOf :EventAndState ;
  rdfs:label "Impact and Contact"@en ;
  :definition "An act of coming into physical contact/touching, which may be forceful (e.g., an impact)"@en ;
  :wordnet_noun_synset "07316161", "07316568", "07353152", "00047263" ;
  :wordnet_verb_synset "01208316", "01208838" .

:InclusionAttachmentAndUnification a owl:Class ;
  rdfs:subClassOf :EventAndState ;
  rdfs:label "Inclusion, Attachment and Unification"@en ;
  :definition "An act of fastening, joining, grouping or bringing together, etc."@en ;
  :wordnet_noun_synset "00379793", "14441799", "00148415", "00380230", "01215372" ; 
  :wordnet_verb_synset "00193830", "01464634", "00187671", "00368338", "00244010", "01387824" . 

:Increase a owl:Class ;
  rdfs:subClassOf :Change ;
  rdfs:label "Increase"@en ;
  :definition "Indicates a growth/rise in the availability/count/amount of something or in the demeanor/behavior of an Agent. The pace of the Increase can be described using an associated instance of :AssessmentMeasurement (associated using the :has_measurement property)."@en ;
  :noun_synonym  "rise", "expansion", "escalation", "addition", "broadening", "rallying", "increasing", "rising", "expanding", "escalating", "adding", "broadening", "extending", "surge", "amplification", "maximization", "maximisation", "maximation", "strengthening" ;
